[colors.bright]
black = "#928374"
blue = "#458588"
cyan = "#458588"
green = "#98971a"
magenta = "#b16286"
red = "#cc241d"
white = "#fbf1c7"
yellow = "#d79921"

[colors.normal]
black = "#282828"
blue = "#83a598"
cyan = "#83a598"
green = "#b8bb26"
magenta = "#d3869b"
red = "#fb4934"
white = "#fbf1c7"
yellow = "#fabd2f"

[colors.primary]
background = "#282828"
foreground = "#fbf1c7"

[colors.selection]
background = "#424242"
text = "#fbf1c7"

[cursor]
style = "Underline"
unfocused_hollow = true

[env]
TERM = "xterm-256color"

[font]
size = {{ .alacritty_font_size }}

[font.bold]
style = "Bold"

[font.bold_italic]
style = "Bold Italic"

[font.italic]
style = "Italic"

[font.normal]
family = "JetBrainsMono Nerd Font"
style = "SemiBold"

[font.offset]
x = 0
y = -1

[[keyboard.bindings]]
action = "Paste"
key = "Insert"
mods = "Shift"

[[keyboard.bindings]]
chars = "gc"
key = "Slash"
mods = "Control"

[scrolling]
history = 5000

[window]
decorations = "full"
decorations_theme_variant = "Dark"
title = "Alacritty"
option_as_alt = "both"

[window.class]
general = "Alacritty"
instance = "Alacritty"

[window.padding]
x = 5
y = 5


